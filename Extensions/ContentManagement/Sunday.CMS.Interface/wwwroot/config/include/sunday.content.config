<configuration>
  <pipelines>
    <cms.contentTree.getRoots>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.ContentTrees.GetRoots, Sunday.ContentManagement.Implementation"></processor>
    </cms.contentTree.getRoots>
    <cms.contentTree.getChilds>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.ContentTrees.GetOrganizationsChilds, Sunday.ContentManagement.Implementation"></processor>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.ContentTrees.GetWebsiteChilds, Sunday.ContentManagement.Implementation"></processor>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.ContentTrees.GetContentChilds, Sunday.ContentManagement.Implementation"></processor>
    </cms.contentTree.getChilds>
    <cms.contentTree.getContextMenu>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.ContentTrees.GetWebsiteContextMenu, Sunday.ContentManagement.Implementation"></processor>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.ContentTrees.GetContentContextMenu, Sunday.ContentManagement.Implementation"></processor>
    </cms.contentTree.getContextMenu>
    <cms.content.beforeCreate>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.Contents.ValidateAndFormalizeContentName, Sunday.ContentManagement.Implementation"></processor>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.Contents.ResolveContentPath, Sunday.ContentManagement.Implementation"></processor>
    </cms.content.beforeCreate>
    <cms.content.beforeUpdate>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.Contents.ValidateAndFormalizeContentName, Sunday.ContentManagement.Implementation"></processor>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.Contents.ResolveContentPath, Sunday.ContentManagement.Implementation"></processor>
    </cms.content.beforeUpdate>
    <cms.content.afterUpdate>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.Contents.UpdateContentCache, Sunday.ContentManagement.Implementation"></processor>
    </cms.content.afterUpdate>
    <cms.content.beforeDelete>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.Contents.UpdateContentCache, Sunday.ContentManagement.Implementation"></processor>
    </cms.content.beforeDelete>
    <!-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>-->
    <cms.contentTree.getByPath>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.ContentTrees.GetTreeByPath, Sunday.ContentManagement.Implementation"></processor>
    </cms.contentTree.getByPath>
    <cms.contentTree.getByQuery>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.ContentTrees.GetContentTreeByQuery, Sunday.ContentManagement.Implementation"></processor>
    </cms.contentTree.getByQuery>
    <cms.templates.filter>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.Templates.FilterTemplateAccess, Sunday.ContentManagement.Implementation"></processor>
    </cms.templates.filter>
    <cms.renderings.filter>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.Renderings.FilterRenderingAccess, Sunday.ContentManagement.Implementation"></processor>
    </cms.renderings.filter>
    <cms.content.getSiblingsOrder>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.Contents.ResolveSiblingsOrder, Sunday.ContentManagement.Implementation"></processor>
    </cms.content.getSiblingsOrder>
    <cms.entity.getDependants>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.CacheDependencies.GetContentDependants, Sunday.ContentManagement.Implementation"></processor>
    </cms.entity.getDependants>
    <cms.entity.getMasters>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.CacheDependencies.GetContentMasters, Sunday.ContentManagement.Implementation"></processor>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.CacheDependencies.GetTemplateMasters, Sunday.ContentManagement.Implementation"></processor>
    </cms.entity.getMasters>
  </pipelines>
</configuration>