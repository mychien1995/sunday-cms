<configuration>
  <pipelines>
    <configureServices>
      <processor type="Sunday.Core.Framework.Pipelines.ConfigureServices.AutomapBootstrap, Sunday.Core.Framework"></processor>
      <processor type="Sunday.Core.Media.Pipelines.ConfigureServices.ResolveBlobProvider, Sunday.Core.Media"></processor>
      <processor type="Sunday.Core.Framework.Pipelines.ConfigureServices.ResolveTcpRemoteEventHandler, Sunday.Core.Framework"></processor>
      <processor type="Sunday.Core.Framework.Pipelines.ConfigureServices.ResolveCachingProvider, Sunday.Core.Framework"></processor>
      <processor type="Sunday.Core.Framework.Pipelines.ConfigureServices.AddTcpBackgroundService, Sunday.Core.Framework"></processor>
      <processor type="Sunday.ContentManagement.Implementation.Pipelines.ConfigureServices.AddFieldTypesLoader, Sunday.ContentManagement.Implementation"></processor>
      <processor type="Sunday.ContentDelivery.Framework.Pipelines.IncludeProjectParts, Sunday.ContentDelivery.Framework"></processor>
    </configureServices>
    <initialize>
      <processor type="Sunday.Core.Media.Pipelines.BuildApplication.AddBlobPreviewLinkMiddleware, Sunday.Core.Media"></processor>
      <processor type="Sunday.ContentDelivery.Framework.Pipelines.HandleRemoteEvent, Sunday.ContentDelivery.Framework"></processor>
    </initialize>
  </pipelines>
  <templates>
    <fieldTypes>
      <add id="1">Sunday.ContentManagement.FieldTypeHandlers.SingleLineTextHandler, Sunday.ContentManagement</add>
      <add id="2">Sunday.ContentManagement.FieldTypeHandlers.MultilineTextHandler, Sunday.ContentManagement</add>
      <add id="3">Sunday.ContentManagement.FieldTypeHandlers.NumberHandler, Sunday.ContentManagement</add>
      <add id="4">Sunday.ContentManagement.FieldTypeHandlers.RichTextHandler, Sunday.ContentManagement</add>
      <add id="5">Sunday.ContentManagement.FieldTypeHandlers.ImageHandler, Sunday.ContentManagement</add>
      <add id="6">Sunday.ContentManagement.FieldTypeHandlers.LinkHandler, Sunday.ContentManagement</add>
      <add id="7">Sunday.ContentManagement.FieldTypeHandlers.RenderingAreaHandler, Sunday.ContentManagement</add>
      <add id="8">Sunday.ContentManagement.FieldTypeHandlers.DropTreeHandler, Sunday.ContentManagement</add>
      <add id="9">Sunday.ContentManagement.FieldTypeHandlers.MultilistHandler, Sunday.ContentManagement</add>
    </fieldTypes>
  </templates>
  <remoteEvent handler="TCP">
    <listening>9996</listening>
    <publishing></publishing>
  </remoteEvent>
  <caching provider="Sunday.Core.Implementation.InMemoryCacheEngine,Sunday.Core"/>
  <settings>
    <setting name="Sunday.CMS.SharedHosts" value="localhost"></setting>
    <!--<setting name="Sunday.CD.ShowUnpublished" value="true"></setting>-->
  </settings>
</configuration>