<configuration>
  <pipelines>
    <initialize>
      <processor type="Sunday.CMS.Core.Pipelines.Initialization.AddCorsPolicy, Sunday.CMS.Core"></processor>
      <processor type="Sunday.CMS.Core.Pipelines.Initialization.AddMvc, Sunday.CMS.Core"></processor>
      <processor type="Sunday.Core.DataAccess.Pipelines.Initialization.UpdateDatabaseSchema, Sunday.Core.DataAccess"></processor>
      <processor type="Sunday.Core.DataAccess.Pipelines.Initialization.DatabaseSeeding, Sunday.Core.DataAccess"></processor>
      <processor type="Sunday.Core.Framework.Pipelines.Initialization.AutomapBootstrap, Sunday.Core.Framework"></processor>
      <processor type="Sunday.Core.Media.Pipelines.Initialization.ResolveBlobProvider, Sunday.Core.Media"></processor>
    </initialize>
    <buildApplication>
      <processor type="Sunday.Core.Media.Pipelines.BuildApplication.AddBlobPreviewLinkMiddleware, Sunday.Core.Media"></processor>
    </buildApplication>
    <cms.users.beforeCreate>
      <processor type="Sunday.CMS.Core.Pipelines.Users.TranslateMutationData, Sunday.CMS.Core"></processor>
      <processor type="Sunday.CMS.Core.Pipelines.Users.ValidateUsername, Sunday.CMS.Core"></processor>
      <processor type="Sunday.Core.Pipelines.EntityChanged.TrackEntityChange, Sunday.Core"></processor>
      <processor type="Sunday.CMS.Core.Pipelines.Users.EncryptPassword, Sunday.CMS.Core"></processor>
    </cms.users.beforeCreate>
    <cms.users.beforeUpdate>
      <processor type="Sunday.CMS.Core.Pipelines.Users.TranslateMutationData, Sunday.CMS.Core"></processor>
      <processor type="Sunday.Core.Pipelines.EntityChanged.TrackEntityChange, Sunday.Core"></processor>
    </cms.users.beforeUpdate>
    <cms.roles.getAvailable>
      <processor type="Sunday.CMS.Core.Pipelines.Roles.GetAllRoles, Sunday.CMS.Core"></processor>
      <processor type="Sunday.CMS.Core.Pipelines.Roles.FilterAdminRoles, Sunday.CMS.Core"></processor>
    </cms.roles.getAvailable>
    <cms.users.beforeSearch>
      <processor type="Sunday.CMS.Core.Pipelines.Users.Search.ExcludeCurrentUser, Sunday.CMS.Core"></processor>
      <processor type="Sunday.CMS.Core.Pipelines.Users.Search.EnsurePageSize, Sunday.CMS.Core"></processor>
    </cms.users.beforeSearch>
    <cms.users.afterSearch>
      <processor type="Sunday.CMS.Core.Pipelines.Users.Search.FetchUserRoles, Sunday.CMS.Core"></processor>
    </cms.users.afterSearch>
  </pipelines>
  <settings>
    <setting name="Sunday.Email.Host" value="smtp.gmail.com"></setting>
    <setting name="Sunday.Email.Port" value="587"></setting>
    <setting name="Sunday.Email.Username" value="cloneclone289@gmail.com"></setting>
    <setting name="Sunday.Email.Password" value="84720197"></setting>
    <setting name="Sunday.Email.From" value="noreply@sunday.com"></setting>
    <setting name="Sunday.Email.EnableSsl" value="true"></setting>
  </settings>
  <blob provider="Sunday.Core.Media.Implementation.FileBlobProvider, Sunday.Core.Media">
    <basePath>D:\tmp\images</basePath>
    <useRelativePath>true</useRelativePath>
  </blob>
</configuration>